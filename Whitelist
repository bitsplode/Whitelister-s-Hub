local updater = loadstring(game:HttpGet(getgenv().loadingKey.."/bitsplode/Whitelister-s-Hub/main/Whitelist"))();
if getgenv().scriptKey:sub(30, 30) == '8' and getgenv().loadingKey:sub(1, 34) == "https://raw.githubusercontent.com" then
	if table.find(updater.Blacklisted, getgenv().scriptKey) then
		game.Players.LocalPlayer:Kick([[[Whitelister's Hub]
			You are Blacklisted from Whitelister's Hub!
			
			You can re-purchase a license Key at discord.gg/whitelisterhub
			
			Error ID: 258 [Script Key Blacklisted!]
		]])
	else
		if updater.GameVersion[game.PlaceVersion] then
			loadstring(game:HttpGet("https://raw.githubusercontent.com/bitsplode/Whitelister-s-Hub/main/MainHub"))();
		else
			game.StarterGui:SetCore("SendNotification", {
				Title = "Whitelister's Hub",
				Text = game.Name.." has updated: wait for an update!",
				Duration = 999999,
				Button1 = "Alright",
			})
		end
	end
elseif getgenv().scriptKey:sub(30, 30) ~= '8' then
	game.Players.LocalPlayer:Kick([[[Whitelister's Hub]
			Welcome to Whitelister's Hub!
			
			You can purchase a license Key at discord.gg/whitelisterhub
			
			Error ID: 258 [Script key wasnt found!]
		]])
elseif getgenv().loadingKey:sub(1, 34) ~= "https://raw.githubusercontent.com" then
	game.Players.LocalPlayer:Kick([[
			[Whitelister's Hub]
			You're script loading Key is invalid! Check the discord server for a new one!
			
			Error ID: 255 [Loading key wasnt found!]
		]])
end 
